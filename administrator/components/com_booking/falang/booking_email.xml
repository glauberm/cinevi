<?xml version="1.0" ?>
<falang type="contentelement">
  <name>Booking E-mail Template</name>
  <author>Artio s.r.o.</author>
  <version>1.0.0</version>
  <description>Definition for the Booking e-mail template</description>
  <reference>
  	<table name="booking_email">
  		<field type="referenceid" name="id" translate="0">ID</field>
  		<field type="titletext" name="subject" translate="1">E-mail Subject</field>
		<field type="htmltext" name="body" translate="1">E-mail Body</field>
        <field type="textarea" name="sms" translate="1">SMS Body</field>
  	</table>
  </reference>
   <translationfilters>
  	  <keyword>subject</keyword>
  	  <published>published</published>
   </translationfilters>
</falang>