<?xml version="1.0" ?>
<falang type="contentelement">
  <name>Booking Subject</name>
  <author>Artio s.r.o.</author>
  <version>1.0.0</version>
  <description>Definition for the Booking component subject</description>
  <reference>
  	<table name="booking_subject">
  		<field type="referenceid" name="id" translate="0">ID</field>
  		<field type="titletext" name="title" translate="1">Title</field>
		<field type="text" name="alias" translate="1" posthandler="filterTitle">Alias</field>
  		<field type="htmltext" name="introtext" translate="1" ebuttons="readmore">Introtext</field>
  		<field type="htmltext" name="fulltext" translate="1" ebuttons="readmore">Fulltext</field>
  	</table>
  </reference>
   <translationfilters>
  	  <keyword>title</keyword>
  	  <published>published</published>
  	  <changed>created|modified</changed>
  </translationfilters>
</falang>