<?xml version="1.0" ?>
<falang type="contentelement">
  <name>Booking Supplement</name>
  <author>Artio s.r.o.</author>
  <version>1.0.0</version>
  <description>Definition for the Booking component object supplement</description>
  <reference>
  	<table name="booking_supplement">
  		<field type="referenceid" name="id" translate="0">ID</field>
  		<field type="titletext" name="title" translate="1">Title</field>
		<field type="textarea" name="description" translate="1">Description</field>
        <field type="textarea" name="options" translate="1">Options</field>
  	</table>
  </reference>
   <translationfilters>
  	  <keyword>title</keyword>
  	  <bookitsubject>subject</bookitsubject>
  	  <published>published</published>
  	  <changed>created|modified</changed>
  </translationfilters>
</falang>